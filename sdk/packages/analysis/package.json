{
  "name": "@schillinger-sdk/analysis",
  "version": "2.0.0",
  "description": "Schillinger System Analysis - Music analysis and pattern recognition tools",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src --ext .ts",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^20.10.0",
    "eslint": "^8.54.0",
    "typescript": "^5.3.0",
    "vitest": "^3.2.4"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./reverse-analysis": {
      "import": "./dist/reverse-analysis/index.js",
      "types": "./dist/reverse-analysis/index.d.ts"
    },
    "./reverse-analysis/*": {
      "import": "./dist/reverse-analysis/*.js",
      "types": "./dist/reverse-analysis/*.d.ts"
    }
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  }
}
