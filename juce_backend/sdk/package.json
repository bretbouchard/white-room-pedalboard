{
  "name": "@schillinger-sdk/sdk-monorepo",
  "version": "2.0.0",
  "description": "Schillinger System SDK - Comprehensive music composition toolkit",
  "packageManager": "npm@9.5.1",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "npm run build:sequential",
    "build:sequential": "npm run build:shared && npm run build:core && npm run build:analysis && npm run build:admin && npm run build:gateway && npm run build:generation && npm run build:audio",
    "build:shared": "cd packages/shared && npm run build",
    "build:core": "cd packages/core && npm run build",
    "build:analysis": "cd packages/analysis && npm run build",
    "build:admin": "cd packages/admin && npm run build",
    "build:gateway": "cd packages/gateway && npm run build",
    "build:generation": "cd packages/generation && npm run build",
    "build:audio": "cd packages/audio && npm run build",
    "test": "npm run test:cleanup && vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:cleanup": "./scripts/cleanup-test-processes.sh",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "test:integration:api": "vitest run --config vitest.integration.config.ts tests/integration/api-integration.test.ts tests/integration/auth-integration.test.ts",
    "test:integration:websocket": "vitest run --config vitest.integration.config.ts tests/integration/websocket.test.ts",
    "test:integration:cross-platform": "vitest run --config vitest.integration.config.ts tests/integration/cross-platform.test.ts",
    "test:integration:environment": "vitest run --config vitest.integration.config.ts tests/integration/environment.test.ts",
    "test:integration:full": "./run-integration-tests.sh",
    "test:integration:typescript": "./run-integration-tests.sh --typescript-only",
    "test:integration:python": "./run-integration-tests.sh --python-only",
    "test:integration:swift": "./run-integration-tests.sh --swift-only",
    "test:integration:cpp": "./run-integration-tests.sh --cpp-only",
    "test:integration:parallel": "./run-integration-tests.sh --parallel",
    "test:integration:live": "./run-integration-tests.sh --no-mock",
    "test:performance": "vitest run --config vitest.performance.config.ts",
    "test:performance:mathematical": "vitest run --config vitest.performance.config.ts tests/performance/mathematical-operations.test.ts",
    "test:performance:caching": "vitest run --config vitest.performance.config.ts tests/performance/caching-operations.test.ts",
    "test:performance:load": "vitest run --config vitest.performance.config.ts tests/performance/load-testing.test.ts",
    "test:performance:profiling": "vitest run --config vitest.performance.config.ts tests/performance/profiling.test.ts",
    "test:performance:full": "./run-performance-tests.sh",
    "lint": "eslint packages/{shared,core,analysis,gateway,generation,admin,audio}/src/**/*.ts",
    "lint:fix": "eslint packages/{shared,core,analysis,gateway,generation,admin,audio}/src/**/*.ts --fix",
    "lint:shared": "eslint packages/shared/src/**/*.ts",
    "lint:core": "eslint packages/core/src/**/*.ts",
    "lint:analysis": "eslint packages/analysis/src/**/*.ts",
    "lint:gateway": "eslint packages/gateway/src/**/*.ts",
    "lint:generation": "eslint packages/generation/src/**/*.ts",
    "lint:admin": "eslint packages/admin/src/**/*.ts",
    "lint:audio": "eslint packages/audio/src/**/*.ts",
    "lint:all": "npm run lint:shared && npm run lint:core && npm run lint:analysis && npm run lint:gateway && npm run lint:generation && npm run lint:admin && npm run lint:audio",
    "clean": "npm run clean --workspaces",
    "dev": "npm run dev --workspaces --if-present",
    "type-check": "tsc -b tsconfig.build.json --force",
    "type-check:shared": "tsc --noEmit -p packages/shared/tsconfig.json",
    "type-check:core": "tsc --noEmit -p packages/core/tsconfig.json",
    "type-check:analysis": "tsc --noEmit -p packages/analysis/tsconfig.json",
    "type-check:gateway": "tsc --noEmit -p packages/gateway/tsconfig.json",
    "type-check:generation": "tsc --noEmit -p packages/generation/tsconfig.json",
    "type-check:admin": "tsc --noEmit -p packages/admin/tsconfig.json",
    "type-check:audio": "tsc --noEmit -p packages/audio/tsconfig.json",
    "type-check:all": "npm run type-check:shared && npm run type-check:analysis && npm run type-check:core && npm run type-check:gateway && npm run type-check:generation && npm run type-check:admin && npm run type-check:audio",
    "quality": "npm run type-check:all && npm run lint:all",
    "quality:fix": "npm run type-check:all && npm run lint:fix",
    "start:test-server": "node scripts/start-test-server.js",
    "version:current": "./scripts/version-manager.sh current",
    "version:validate": "./scripts/version-manager.sh validate",
    "version:sync": "./scripts/version-manager.sh sync",
    "version:bump:major": "./scripts/version-manager.sh --bump major",
    "version:bump:minor": "./scripts/version-manager.sh --bump minor",
    "version:bump:patch": "./scripts/version-manager.sh --bump patch",
    "version:bump:prerelease": "./scripts/version-manager.sh --bump prerelease",
    "release:major": "./scripts/release.sh --type major",
    "release:minor": "./scripts/release.sh --type minor",
    "release:patch": "./scripts/release.sh --type patch",
    "release:prerelease": "./scripts/release.sh --type prerelease --prerelease",
    "publish:npm": "./scripts/publish-npm.sh",
    "publish:pypi": "./scripts/publish-pypi.sh",
    "publish:swift": "./scripts/publish-swift.sh",
    "publish:cpp": "./scripts/publish-cpp.sh",
    "publish:all": "./scripts/publish-all.sh",
    "compatibility:test": "./scripts/compatibility-test.sh",
    "deprecation:add": "./scripts/deprecation-manager.sh add",
    "deprecation:list": "./scripts/deprecation-manager.sh list",
    "deprecation:remove": "./scripts/deprecation-manager.sh remove",
    "deprecation:guide": "./scripts/deprecation-manager.sh generate",
    "prepare": "if [ \"$CI\" = \"true\" ] || [ \"$VERCEL\" = \"1\" ] || [ \"$NODE_ENV\" = \"production\" ]; then echo 'Skipping husky in CI'; else husky; fi"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.3.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.19.11",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "dotenv": "^17.2.1",
    "eslint": "^8.57.1",
    "express": "^5.1.0",
    "husky": "^9.1.7",
    "prettier": "^3.1.0",
    "turbo": "^2.5.6",
    "typescript": "^5.3.0",
    "vite-tsconfig-paths": "^4.3.0",
    "vitest": "^3.2.4",
    "ws": "^8.18.3"
  },
  "dependencies": {
    "@genkit-ai/mcp": "^1.16.1",
    "@types/cors": "^2.8.19",
    "cors": "^2.8.5",
    "helmet": "^8.1.0",
    "joi": "^18.0.1",
    "pg": "^8.11.3",
    "redis": "^4.6.10",
    "winston": "^3.11.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
